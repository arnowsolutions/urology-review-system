{
    "name": "urology-review-system",
    "version": "1.0.0",
    "description": "Urology Residency Review System",
    "scripts": {
        "start": "parcel public/index.html --open",
        "build": "parcel build public/index.html --dist-dir dist",
        "build:frontend": "parcel build public/index.html --dist-dir dist",
        "type-check": "tsc --noEmit",
        "lint": "eslint src --ext .ts,.tsx",
        "test:api:local": "API_BASE_URL=http://localhost:3001/api node scripts/testApiConnectivity.cjs",
        "test:api:production": "API_BASE_URL=/api node scripts/testApiConnectivity.cjs"
    },
    "dependencies": {
        "lucide-react": "^0.294.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/node": "^24.5.2",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "autoprefixer": "^10.4.0",
        "parcel": "^2.9.0",
        "postcss": "^8.4.0",
        "tailwindcss": "^3.3.0",
        "typescript": "^5.0.0"
    },
    "keywords": [
        "urology",
        "review",
        "residency"
    ],
